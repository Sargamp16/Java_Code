
public class Main
{
	public static void main(String[] args) {
		int i,j;
		for (i=1;i<6;i++)
		{
		    
		    for(j=1;j<i+1;j++)
		    {
	     	 if(j%2!=0)
		     System.out.print(j);
		     else
		     System.out.print("_");
		    
	}
	System.out.println(); // "Function is used to print something in the new line"
		}

}
}
